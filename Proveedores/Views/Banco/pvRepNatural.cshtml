@model List<NATURAL_DETALLE>

@if ( Model.Count  > 0)
{
    
    <h5>Personas Naturales</h5>
    <table class="table table-sm">
        <thead>
            <tr>
                <th>Nombr</th>
                <th>Nit</th>
                <th>Profesion</th>
                <th>Email</th>
                <th>Ver más</th>
            </tr>
        </thead>
        <tbody>
            @foreach (NATURAL_DETALLE natural in Model)
            {
                <tr>
                    <td>@natural.NOM_EMPRESA </td>
                    <td>@natural.NIT</td>
                    <td>@natural.NOM_PROFESION</td>
                    <td>@natural.EMAIL</td>

                    <td>
                        <a href="@Url.Action("TotalNatural",new { id= natural.ID_EMPRESA })" class="btn btn-primary btn-sm">
                            Ver más...
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <div class="text-left">No se encontraron coincidencias para esta busqueda !!!</div>
}